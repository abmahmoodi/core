<% if @error %>
    <%= render 'shared/modal_hide_and_flash' %>
<% else %>
    <% if !@error_keys %>
        <%= render 'shared/modal_hide_and_flash' %>
        $('#user').html("<%= j render 'list' %>");
    <% else %>
        require(["/assets/rw/ujs_error_handler"],function(){
            HandleError('rw_user', <%= raw @error_keys %>, <%= raw @complete_errors %>);
        });
    <% end %>
<% end %>

require(["/assets/rw/hide_flash"],function(){
    hideFlash();
});

