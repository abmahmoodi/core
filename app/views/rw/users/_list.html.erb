<table id="user_list" class="table table-striped table-bordered table-hover table-checkable order-column"
       data-source="<%= rw.user_datatable_path(format: 'json') %>">
  <thead>
  <tr>
    <th></th>
    <th><%= t(:'users.code') %></th>
    <th><%= t(:'users.email') %></th>
    <th><%= t(:'users.user_type') %></th>
    <th><%= "#{t(:'profiles.first_name')} - #{t(:'profiles.last_name')}" %></th>
    <th><%= t(:'users.last_login') %></th>
    <th><%= t(:'users.login_count') %></th>
    <th><%= t(:'users.agent_city') %></th>
    <th></th>
  </tr>

  </thead>
  <tbody>
  </tbody>
</table>

<script>
    require(["/assets/rw/run_datatable"], function () {
        RunDatatable('user_list', null, 8);
    });
</script>